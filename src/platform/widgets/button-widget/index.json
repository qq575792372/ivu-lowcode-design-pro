{
  "type": "button",
  "label": "按钮",
  "widgets": [],
  "props": {
    "name": "",
    "value": "button",
    "className": "custom-button-primary"
  },
  "events": [
    {
      "name": "custom-click",
      "label": "custom-click",
      "args": [
        "widget"
      ],
      "code": "console.log('custom-click的点击事件',widget);",
      "action": [
        "$globalActions.globalCustomAction1"
      ]
    }
  ],
  "actions": [
    {
      "name": "customButtonAction",
      "label": "自定义按钮动作",
      "enable": true,
      "code": "console.log('触发自定义按钮动作',widget);"
    }
  ]
}
